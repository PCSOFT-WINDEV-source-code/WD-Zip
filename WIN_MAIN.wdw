      PROCEDURE Search()
      // Find a file in an archive
      
      IF zipExist(gMyArchive)=False THEN
      	Error(<§$00a5§>)
      	RETURN
      END
      
      sSoughtFile	is string
      nFileSubscript		is int
      nTableSubscript		is int
      
      // Select the file to find
      sSoughtFile = Open(WIN_SEARCH)
      
      IF sSoughtFile<>"" THEN
      	
      	TableSelectMinus(TABLE_ARCHIVE)
      	
      	// Find a file in the current archive
      	nFileSubscript = zipFindFile(gMyArchive, sSoughtFile, False)
      	IF nFileSubscript = -1 THEN
      		Info(StringBuild(<§$003e§>, sSoughtFile))
      	ELSE
      		nTableSubscript = TableSeek(TABLE_ARCHIVE.COL_ARCHIVE_SUBSCRIPT,nFileSubscript)
      		IF nTableSubscript<>-1
      			TableSelectPlus(TABLE_ARCHIVE, nFileSubscript)
      			TablePosition(TABLE_ARCHIVE, nFileSubscript)
      		END
      	END
      END
      
      
