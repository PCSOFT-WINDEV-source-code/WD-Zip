      // Summary: Used to open an existing archive
      // Parameters:
      //	sArchive (ANSI string): Path of existing archive
      // Return value:
      // 	boolean: True if the opening was successful, False otherwise
      //
      PROCEDURE OpenArchive(sArchive is string) : boolean
      
      // Inform the user that a process is in progress
      HourGlass(True)
      PROGBAR_CurrentProcess..Visible =True
      PROGBAR_CurrentProcess = 0
      
      // Close the opened archive (if any)
      CloseArchive()
      
      // Check whether the archive is read only
      IF Position(fAttribute(sArchive), "R")>0 THEN
      	MyWindow..Title += " "+<§@163ca2ca012797500007§>
      END
      
      // The opening failed? 
      IF ArchiveOpenArchive(sArchive) = False THEN
      	HourGlass(False)
      	PROGBAR_CurrentProcess..Visible =False
      	RESULT False
      END
      
      
      // Initialize the progress bar indicating that a process is in progress
      PROGBAR_CurrentProcess..MaxValue = zipNbFile(gMyArchive)
      PROGBAR_CurrentProcess= 0
      
      // Display the archive
      RefreshDisplay
      
      // Path of current archive
      gsArchive = sArchive
      
      // Update the title of the window
      MyWindow..Title = ProjectInfo(piProjectName)+" - "+fExtractPath(sArchive, fFileName+fExtension)
      
      // Deselect the table
      TableSelectMinus(TABLE_ARCHIVE)
      
      // Update the messages for the user
      STC_MESSAGE1 = StringBuild(<§@163ca2ca012797500003§>, gMyArchive.File..Occurrence , LengthToString(zipSize(gMyArchive)))
      STC_MESSAGE2 = StringBuild(<§@163ca2ca012797500004§>, 0, LengthToString(0))
      STC_ArchivePath = Left(gsArchive,Position(gsArchive,fSep,Length(gsArchive),FromEnd)-1)
      STC_ArchiveName = ExtractString(gsArchive,1,fSep,FromEnd)
      
      
      // Don't accept drag and drop for the .rar files
      IF gMyArchive..ArchiveFormat=zipFormatRAR THEN
      	// No drop from the Windows explorer
      	ExplorerAccept(False,WIN_MAIN..Name)
      	STC_MESSAGE3 = ""
      ELSE
      	// Other archive format
      	// Drop allowed from the Windows explorer
      	ExplorerAccept(True,WIN_MAIN..Name)
      	STC_MESSAGE3 = <§@163ca2ca012797500005§>
      END
      
      // Status of buttons
      UpdStatusButtons()
      
      // Indicate that the process is ended
      HourGlass(False)
      PROGBAR_CurrentProcess..Visible =False
      
      RESULT True
