      // Path of current archive
      gsArchive = sFileArchive
      
      // Display the information to the user
      STC_MESSAGE1 = StringBuild(<§@163ca2ca012797500003§>, 0, LengthToString(0))
      STC_MESSAGE2 = StringBuild(<§@163ca2ca012797500004§>, 0, LengthToString(0))
      STC_MESSAGE3 = <§@163ca2ca012797500005§>
      
      // Fill the information about the archive (at the top)
      STC_ArchivePath = Left(gsArchive,Position(gsArchive,fSep,Length(gsArchive),FromEnd)-1)
      STC_ArchiveName = ExtractString(gsArchive,1,fSep,FromEnd)
      
      // Change the title of the window
      MyWindow..Title = ProjectInfo(piProjectName)+" - "+fExtractPath(sFileArchive, fFileName+fExtension)
      
      // Status of buttons
      UpdStatusButtons()
      
      // Accept drag and drop from the explorer
      ExplorerAccept(True,WIN_MAIN..Name)
      
      
      RESULT True
