      // Summary: Creates a slef-extracting exe file 
      // Parameters:
      //	psEmulatingDir (default value=""): Extraction directory
      // Return value:
      // 	None
      //
      PROCEDURE CreateSelfExtracting(psControllingDir="")
      // Create the self-extracting archive
      // psEmulatingDir is filled in emulating mode only
      
      // Check whether the archive exists
      IF zipExist(gMyArchive)=False THEN
      	Error(<§$00a5§>)
      	RETURN
      END
      
      // If the archive is multi-file
      IF zipIsMulti(gMyArchive) THEN
      	Error(<§$00a6§>)
      	RETURN
      END
      
      // If the option is not allowed
      IF gMyArchive..ArchiveFormat IN (zipFormat7Z, zipFormatCAB, zipFormatRAR,zipFormatTAR,zipFormatTARGZ) THEN
      	Error(<§$00aa§>)
      	RETURN
      END
      
      // default "Uncompress in" directory
      sExtractionDir is string = psControllingDir
      sRes is string
      bGUI is boolean
      
      // If no directory
      IF sExtractionDir="" THEN
      	sRes = Open(WIN_DEFAULT_EXTRACTION_DIRECTORY)
      	IF sRes="" RETURN	
      	sExtractionDir = ExtractString(sRes,1,TAB)
      	bGUI = NOT Val(ExtractString(sRes,2,TAB))
      	
      ELSE
      	bGUI = False
      END
      
      // Name of the exe
      sExeName is string
      sExeName = fExtractPath(gsArchive,fDrive+fDirectory+fFileName)+".exe"
      IF fFileExist(sExeName) THEN
      	IF YesNo(StringBuild(<§$0089§>,sExeName),<§$008a§>)=No THEN
      		RETURN
      	END
      END
      
      // Process started
      HourGlass(True)
      PROGBAR_CurrentProcess..Visible =True
      PROGBAR_CurrentProcess = 0
      
      nResult is int
      IF bGUI THEN
      	nResult = zipCreateExe(gMyArchive, sExeName, zipWithGUI, "WDZip.ico", sExtractionDir,PROGBAR_CurrentProcess)
      ELSE
      	nResult = zipCreateExe(gMyArchive, sExeName, zipDefaultDir+zipTree, "WDZip.ico", sExtractionDir,PROGBAR_CurrentProcess)
      END
      IF nResult=0 THEN
      	Info(<§$0072§>,sExeName)
      ELSE
      	Error(<§$0073§>, zipMsgError(nResult))	
      END
      
      // End of process
      HourGlass(False)
      PROGBAR_CurrentProcess..Visible =False
