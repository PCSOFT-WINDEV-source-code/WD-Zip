      // Summary: Function automatically called during the explorer drag and drop
      // Parameters:
      //	None
      // Return value:
      // 	boolean: // 	None
      //
      PROCEDURE DragAndDrop()
      
      i is int
      nNbElements is int
      sElement is string
      
      // Retrieve the number of selected elements (files + directories)
      nNbElements = ExplorerRetrieve(_EVE.wParam)
      
      // special case 
      IF nNbElements=1 THEN
      	sElement = ExplorerRetrieve(_EVE.wParam, 1)
      	
      	// Are we in the drag and drop from the table? 
      	IF sElement = SET_DragAndDrop_Explorer.sFileObserved THEN
      		SET_DragAndDrop_Explorer.EndDND()
      		RESULT False
      	END
      	
      	IF fExtractPath(sElement,fExtension)~=".wdz" _OR_ fExtractPath(sElement,fExtension)~=".zip" _OR_ ...
      		fExtractPath(sElement,fExtension)~=".rar" _OR_ fExtractPath(sElement,fExtension)~=".cab" THEN
      		IF zipExist(gMyArchive)=True THEN
      			IF YesNo(StringBuild(<§$0086§>, fExtractPath(sElement,fFileName+fExtension)),...
      				<§$0087§>, <§$0088§>)=Yes THEN
      				CloseArchive()
      				OpenArchive(sElement)
      				RESULT True
      			END
      		ELSE
      			OpenArchive(sElement)
      			RESULT True
      		END
      	END
      END
      
      
      // Process started
      HourGlass(True)
      PROGBAR_CurrentProcess..Visible =True
      PROGBAR_CurrentProcess = 0
      
      ListDeleteAll(LIST_HIDDEN)
      
      // Browse the elements (files + directories)
      FOR i=1 TO nNbElements
      	
      	sElement = ExplorerRetrieve(_EVE.wParam, i)
      	
      	// if it's a file
      	IF fFileExist(sElement)=True THEN
      		ListAdd(LIST_HIDDEN,sElement)
      	ELSE
      		// lists the directory files	
      		fListFile(sElement+fSep+"*.*", "ListFiles")
      	END
      END
      
      // adds the files found in the hidden list to the archive
      AddFileList()
      
      // End of process
      HourGlass(False)
      PROGBAR_CurrentProcess..Visible =False
      
      UpdStatusButtons()
      
      RESULT True
