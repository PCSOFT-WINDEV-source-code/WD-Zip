      // Summary: Opens the selected file by extracting it into the temporary directory
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE OpenFile()
      
      IF TableSelectCount(TABLE_ARCHIVE)=0 THEN
      	Error(<§$007f§>)
      	RETURN
      END
      
      // Process started
      HourGlass(True)
      PROGBAR_CurrentProcess..Visible =True
      PROGBAR_CurrentProcess = 0
      
      sSavDir is string
      nResult is int
      
      // save the context
      sSavDir = fCurrentDir()
      fMakeDir(fTempPath()+[fSep]+"Extraction")
      fCurrentDir(fTempPath()+[fSep]+"Extraction")
      
      //extracting
      nResult = zipExtractFile(gMyArchive,COL_ARCHIVE_SUBSCRIPT,zipNone,PROGBAR_CurrentProcess)
      
      // Password
      IF nResult = 21 THEN
      	// Asks for the password
      	sPassword is string = Open(WIN_Password)		
      	IF sPassword <> "" THEN
      		zipPassword(gMyArchive,sPassword)	
      		nResult = zipExtractFile(gMyArchive,COL_ARCHIVE_SUBSCRIPT,zipNone,PROGBAR_CurrentProcess)
      	END	
      END
      
      IF nResult=0 THEN
      	sGUID is string = GetGUID(guidRough) + "_" 
      	fRename(fTempPath()+[fSep]+"Extraction"+fSep+COL_NAME,fTempPath()+[fSep]+"Extraction"+fSep+sGUID+COL_NAME)
      	ShellExecute(fTempPath()+[fSep]+"Extraction"+fSep+sGUID+COL_NAME)
      ELSE
      	Error(<§$0094§>, zipMsgError(nResult))
      END
      
      // restore the context
      fCurrentDir(sSavDir)
      
      // End of process
      HourGlass(False)
      PROGBAR_CurrentProcess..Visible =False
