      // Summary: Displays a file found in the archive on the screen in the table 
      // Parameters:
      // 	nSubscript: Subscript in the archive file of the file to display
      // Return value:
      // 	integer: Insertion subscript
      //
      FUNCTION DisplayArchiveFile(nSubscript) : int
      
      ////////////////////////////////////////////////////////////////////////////////////////
      // OPTIMIZATION IN DIRECTORY MODE, PERFORM THE CHECKS FIRST
      // USED TO ACCELERATE THE SPEED IF THE ARCHIVE IS STRUCTURED IN FOLDER
      ////////////////////////////////////////////////////////////////////////////////////////
      
      
      // Check whether the file must be processed
      sFilePath				is string
      sFilePath			= gMyArchive..File[nSubscript]..Name
      sFileName					is string
      sFileName			= ExtractString(gMyArchive..File[nSubscript]..Name,1,fSep,FromEnd)
      
      // Are we in directory mode?
      IF gbDirectoryModeDisplay = True THEN
      	
      	IF StringCount(sFilePath,fSep) > gnDisplayLevel THEN
      		// Does the directory already exist?
      		sFileName = ExtractString(sFilePath,(gnDisplayLevel+1),fSep,FromBeginning)
      		nSubscriptFind is int = TableSeek(COL_NAME,sFileName)
      		IF nSubscriptFind>0 THEN
      			RESULT nSubscriptFind
      		END
      	END
      	
      	// The file or directory contains the current path? If not, don't display it
      	IF Contains(sFilePath,gsCurrentDirectoryPath) = False AND gsCurrentDirectoryPath<>"" THEN
      		RESULT -1
      	END
      	
      	IF StringCount(sFilePath,fSep) < gnDisplayLevel THEN
      		// Don't display the file because it is in the previous folders
      		RESULT -1
      	END	
      END
      
      
      
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      // END OF CHECKS/START OF STANDARD ALGORITHM
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      
      
      // Display the details of a file found in the current archive
      
      sFileInfo	 			is string
      nInitialSize			is int
      nCompressedSize			is int
      dCreationDateTime			is datetime
      dModificationDateTime		is datetime
      dAccessDateTime				is datetime
      sFileAttribute			is string
      nRate						is int
      sIcon						is string
      nAdditionSubscript 				is int
      
      sFileInfo		= zipInfoFile(gMyArchive, nSubscript)
      sFileName			= ExtractString(gMyArchive..File[nSubscript]..Name,1,fSep,FromEnd)
      
      IF sFileName="" RESULT -1
      
      sFilePath			= gMyArchive..File[nSubscript]..Name
      nInitialSize		= gMyArchive..File[nSubscript]..Size
      nCompressedSize		= gMyArchive..File[nSubscript]..CompressedSize
      dCreationDateTime		= gMyArchive..File[nSubscript]..CreationDate
      dModificationDateTime	= gMyArchive..File[nSubscript]..ModificationDate
      dAccessDateTime			= gMyArchive..File[nSubscript]..LastAccessDate
      sFileAttribute		= gMyArchive..File[nSubscript]..FileAttribute
      
      // Check the information
      IF DateValid(dCreationDateTime) = False 		THEN dCreationDateTime = DateSys()+TimeSys()
      IF DateValid(dModificationDateTime) = False 	THEN dModificationDateTime = DateSys()+TimeSys()
      IF DateValid(dAccessDateTime) = False 			THEN dAccessDateTime = DateSys()+TimeSys()
      
      // special case: uncompressible files
      IF nCompressedSize>nInitialSize THEN
      	nCompressedSize=nInitialSize
      END
      
      // calculate the compression rate
      nRate = nCalculateRate(nInitialSize, nCompressedSize)
      
      // retrieves the icon associated with a file
      sExtension is string = fExtractPath(sFileName,fExtension)
      IF gaaIcon[sExtension]..Empty = True THEN
      	gaaIcon[sExtension] = GetAssociatedIcon(sFileName)
      END
      
      sIcon = gaaIcon[sExtension]
      
      IF gbDirectoryModeDisplay = True THEN
      	
      	// Get the name of the file
      	sFileName = ExtractString(sFilePath,(gnDisplayLevel+1),fSep,FromBeginning)
      	
      	IF StringCount(sFilePath,fSep) > gnDisplayLevel THEN
      		
      		// Does the directory already exist?
      		IF TableSeek(COL_NAME,sFileName)>0 THEN
      			RESULT TableSeek(COL_NAME,sFileName)
      		END
      		
      		// Displays the directory of current level			
      		nAdditionSubscript = TableAddLine(TABLE_ARCHIVE, nSubscript, "", ...
      		sFileName,...
      		DateToString(dModificationDateTime..Date)+" "+Left(TimeToString(dModificationDateTime..Time),5),...
      		"",... 
      		"", "", "",... 
      		DateToString(dAccessDateTime..Date)+" "+Left(TimeToString(dAccessDateTime..Time),5),...
      		DateToString(dAccessDateTime..Date)+" "+Left(TimeToString(dAccessDateTime..Time),5),...
      		sFileAttribute) 
      		
      		// Apply the image to the control
      		TABLE_ARCHIVE[nAdditionSubscript].COL_ICON.IMG_File = IMG_Directory
      		
      		RESULT nAdditionSubscript
      	END
      END
      
      nAdditionSubscript = TableAddLine(TABLE_ARCHIVE, nSubscript, "", ...
      sFileName,...
      DateToString(dModificationDateTime..Date)+" "+Left(TimeToString(dModificationDateTime..Time),5),...
      sFilePath,... 
      nInitialSize, nCompressedSize, nRate+"%",... 
      DateToString(dAccessDateTime..Date)+" "+Left(TimeToString(dAccessDateTime..Time),5),...
      DateToString(dAccessDateTime..Date)+" "+Left(TimeToString(dAccessDateTime..Time),5),...
      sFileAttribute)
      
      // Apply the image to the control
      TABLE_ARCHIVE[nAdditionSubscript].COL_ICON.IMG_File = sIcon
      
      
      RESULT nAdditionSubscript
