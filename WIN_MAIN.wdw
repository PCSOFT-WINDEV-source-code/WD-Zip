      PROCEDURE WIN_MAIN(gnCommandLineNumber is int=0)
      
      IF NOT InTestMode()
      	WHEN EXCEPTION
      		Error(StringBuild(<§$00a2§>,ProjectInfo(piProjectName)),<§$00a3§>,"",<§$00a4§>,ExceptionInfo())
      		HourGlass(False)
      		ExceptionEnable()
      		RETURN
      	END
      END
      
      GLOBAL
      	gfbExecutable				is boolean	// Boolean indicating whether an executable was opened
      gnDisplayLevel is int = 0
      gsCurrentDirectoryPath is string 
      gaaIcon is ASSOCIATIVE ARRAY of string
      
      LOCAL
      	
      	
      // UPD the interface
      MyWindow..Title = ProjectInfo(piProjectName)
      STC_MESSAGE1 = StringBuild(<§$0077§>, 0, LengthToString(0))
      STC_MESSAGE2 = StringBuild(<§$0078§>, 0, LengthToString(0))
      
      // Status of buttons
      UpdStatusButtons()
      
      // Popup menu of table
      CheckCtxMenu()
      
      // Drag and Drop from the explorer
      ExplorerAccept(True,WIN_MAIN..Name)
      Event("DragAndDrop", "*.", WM_DROPFILES)
      
      
      // Directory mode by default
      CBOX_DirectoryMode = True
      ExecuteProcess(CBOX_DirectoryMode,trtChange)
      
   -
