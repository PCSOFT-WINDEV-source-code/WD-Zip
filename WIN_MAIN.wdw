      // Summary: Splits an archive into several small volumes
      // Parameters:
      //	psForEmulating (default value=""): Directory for the split
      // Return value:
      // 	None
      //
      PROCEDURE SplitArchive(psForControlling="")
      nSize is int
      bDelArchive is boolean
      sReturn is string
      nResult is int
      sSplitArchive is string
      sTemp is string
      sExtension is string
      i is int
      sSOVName is string
      
      
      // Check whether the archive exists
      IF zipExist(gMyArchive)=False THEN
      	Error(<§$00a5§>)
      	RETURN
      END
      
      // If the archive already is a multi-volume archive
      IF zipIsMulti(gMyArchive)=True THEN
      	Error(<§$008e§>)
      	RETURN
      END
      
      // If the option is not allowed
      IF gMyArchive..ArchiveFormat IN (zipFormat7Z, zipFormatCAB, zipFormatRAR, zipFormatTAR, zipFormatTARGZ) THEN
      	Error(<§$00a9§>)
      	RETURN
      END
      
      
      // Directory or split the archive
      IF psForControlling=""
      	sReturn = Open(WIN_DIVIDE_ARCHIVE)
      ELSE
      	sReturn = psForControlling
      END
      
      
      IF sReturn<>"" THEN
      	// Requested size
      	nSize = ExtractString(sReturn,1,TAB)
      	// ask to delete the initial archive
      	bDelArchive = ExtractString(sReturn,2,TAB)
      	
      	// Check whether the size of sub-volumes is big enough
      	IF nSize<300*1024 THEN
      		Error(<§$009d§>)
      		RETURN
      	END
      	
      	// If no need to split
      	IF zipNbPartNeeded(gMyArchive,nSize)<=1 THEN
      		Info(<§$008f§>)
      		RETURN
      	END
      	
      	// Process started
      	HourGlass(True)
      	PROGBAR_CurrentProcess..Visible =True
      	PROGBAR_CurrentProcess = 0
      	
      	// Temporary name of the split archive
      	sTemp = fExtractPath(gsArchive,fDrive+fDirectory+fFileName)
      	sExtension = fExtractPath(gsArchive,fExtension)
      	sSplitArchive = sTemp+"_d"+sExtension
      	
      	// Split the archive
      	nResult = zipSplit(gMyArchive, sSplitArchive, nSize, False,PROGBAR_CurrentProcess)
      	IF nResult>0 THEN
      		Error(<§$0090§>, zipMsgError(nResult))
      		HourGlass(False)
      		PROGBAR_CurrentProcess..Visible =False
      		RETURN
      	END
      	
      	sSOVName = gsArchive
      	
      	// close the current archive
      	CloseArchive()
      	
      	// Rename the initial archive
      	IF bDelArchive=False THEN
      		fRename(sSOVName, sTemp+"_complete"+sExtension)
      	ELSE
      		fDelete(sSOVName)
      	END
      	
      	// rename the split archives
      	fRename(sTemp+"_d"+sExtension, sTemp+sExtension)
      	
      	// the split files do not have the same extensions (.z01 or .001) in zip mode and in wdz mode
      	IF sExtension~=".wdz" THEN
      		i=1; sSplitArchive = sTemp + "_d." + NumToString(i,"03d")
      		WHILE fFileExist(sSplitArchive)
      			fRename(sSplitArchive, sTemp +"."+ NumToString(i,"03d"))
      			i++; sSplitArchive = sTemp + "_d." + NumToString(i,"03d")
      		END
      	ELSE
      		i=1; sSplitArchive = sTemp + "_d.z" + NumToString(i,"02d")
      		WHILE fFileExist(sSplitArchive)
      			fRename(sSplitArchive, sTemp +".z"+ NumToString(i,"02d"))
      			i++; sSplitArchive = sTemp + "_d.z" + NumToString(i,"02d")
      		END
      	END
      	
      	// open the split archive
      	OpenArchive(sTemp+sExtension)
      	
      	Info(StringBuild(<§$0076§>,zipNbPart(gMyArchive)))
      	
      	// Status of buttons
      	UpdStatusButtons()
      	
      	// End of process
      	HourGlass(False)
      	PROGBAR_CurrentProcess..Visible =False
      END
      
