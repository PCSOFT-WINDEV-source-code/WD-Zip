      // Summary: Function used to create an archive
      // Parameters:
      //	sArchiveFile (ANSI string): path of the archive to create
      // Return value:
      // 	boolean: True if the creation was successful, False otherwise
      //
      PROCEDURE CreateArchive(sFileArchive is string) : boolean
      
      
      // Close the opened archive (if any)
      CloseArchive()
      
      // Check whether the archive was created
      IF ArchiveCreateArchive(sFileArchive) = False THEN
      	RESULT False
      END
      
      // Check the format
      IF gMyArchive..ArchiveFormat=zipFormatTARGZ THEN
      	Info(<§@163ca2ca012797500002§>)
