      // Summary: Groups the multi-archives into a single archive
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE GroupArchive()
      nResult is int
      i is int
      sTemp is string
      sExtension is string
      sGroupedArchive is string
      sSplitArchive is string
      sSOVName is string
      
      // Check whether the archive is not empty
      IF gsArchive="" THEN
      	Error(<§$00a5§>)
      	RETURN
      END
      
      // Check whether the archive is a multi-volume archive
      IF zipIsMulti(gMyArchive)=False THEN
      	Error(<§$008b§>)
      	RETURN
      END
      
      
      // Process started
      HourGlass(True)
      PROGBAR_CurrentProcess..Visible =True
      PROGBAR_CurrentProcess = 0
      
      // Temporary name of the merged archive
      sTemp = fExtractPath(gsArchive,fDrive+fDirectory+fFileName)
      sExtension = fExtractPath(gsArchive,fExtension)
      
      sGroupedArchive = sTemp+"_r"+sExtension
      
      // Merging
      nResult = zipMerge(gMyArchive,sGroupedArchive,PROGBAR_CurrentProcess)
      IF nResult<>0 THEN
      	Error(<§$0075§>, zipMsgError(nResult))	
      	HourGlass(False)
      	PROGBAR_CurrentProcess..Visible = False
      	RETURN
      END
      
      sSOVName = gsArchive
      
      // close the current archive
      CloseArchive()
      
      // Delete the multi-archives
      fDelete(sSOVName)
      
      // The split files do not have the same extensions (.z01 or.001) in zip mode and in wdz mode
      IF sExtension~=".wdz" THEN
      	i=1
      	sSplitArchive = sTemp + "." + NumToString(i,"03d")
      	WHILE fFileExist(sSplitArchive)
      		fDelete(sSplitArchive)
      		i++
      		sSplitArchive = sTemp + "." + NumToString(i,"03d")
      	END
      ELSE
      	i=1
      	sSplitArchive = sTemp + ".z" + NumToString(i,"02d")
      	
      	WHILE fFileExist(sSplitArchive)
      		fDelete(sSplitArchive)
      		i++
      		sSplitArchive = sTemp + ".z" + NumToString(i,"02d")
      	END
      END
      
      // Rename the archive with its final name
      fRename(sTemp+"_r"+sExtension, sTemp+sExtension)
      
      // Open the merged archive
      OpenArchive(sTemp+sExtension)
      
      Info(<§$008d§>, gsArchive)
      
      // Status of buttons
      UpdStatusButtons()
      
      // End of process
      HourGlass(False)
      PROGBAR_CurrentProcess..Visible =False
