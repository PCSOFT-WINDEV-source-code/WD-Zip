      // Summary: Adds a list of files to the archive
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE AddFileList()
      
      i is int
      bHiddenFiles, bSaveDir, bAbsolutePath are booleans
      sReturn is string
      nAddMode is int
      sAttribute is string
      nNbFilesBeforeAdd is int 
      
      
      // The archive exists? 
      IF zipExist(gMyArchive)=False THEN
      	IF YesNo(<§@163ca2ca012797500000§>)=Yes THEN
      		ExecuteProcess(BTN_NEW, trtClick)
      		IF zipExist(gMyArchive)=False THEN RETURN
      	ELSE
      		RETURN
      	END
      END
      
      
      // Ask for the add mode
      sReturn = Open(WIN_MODE_ADDITION)
      IF sReturn<>"" THEN
      	bSaveDir = ExtractString(sReturn,1,TAB)
      	bHiddenFiles = ExtractString(sReturn,2,TAB)
      	bAbsolutePath = ExtractString(sReturn,3,TAB)
      ELSE
      	RETURN
      END
      IF bSaveDir THEN
      	nAddMode = zipDirectory
      ELSE
      	nAddMode = zipNone
      END
      IF bAbsolutePath THEN
      	nAddMode = zipDrive
      END
      
      
      // Retrieve the number of existing files on our archive
      nNbFilesBeforeAdd = gMyArchive..NbFile
      nNbFilesBeforeAdd++
      
      arrFile is array of string
      // For each file to add
      FOR i=1 _TO_ ListCount(LIST_HIDDEN)
      	
      	IF bHiddenFiles=True THEN
      		//sFileList += [CR] + 
      		ArrayAdd(arrFile,LIST_HIDDEN[i])
      	ELSE
      		
      		sAttribute = fAttribute(LIST_HIDDEN[i])
      		// the hidden files and the system files are not added
      		IF Position(sAttribute,"H")+Position(sAttribute,"U")=0 THEN
      			ArrayAdd(arrFile,LIST_HIDDEN[i])
      		END
      	END	
      END
      
      // Start the add process
      HourGlass(True)
      PROGBAR_CurrentProcess..Visible = True
      PROGBAR_CurrentProcess = 0
      
      
      IF ArchiveAddFileList(arrFile,nAddMode,PROGBAR_CurrentProcess) = False THEN
      	RETURN
      END
      
      TABLE_ARCHIVE..DisplayEnabled = False
      FOR i = nNbFilesBeforeAdd _TO_ gMyArchive..NbFile
      	DisplayArchiveFile(i)	
      END
      
      // Refresh the display
      RefreshDisplay
      
      
      TABLE_ARCHIVE..DisplayEnabled = True
      
      // End of addition
      HourGlass(False)
      PROGBAR_CurrentProcess..Visible =False
