      // Summary: UPD the user interface
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE UpdStatusButtons()
      // Status of buttons
      
      // if no archive is opened
      IF zipExist(gMyArchive)=False THEN
      	
      	GR_Buttons..Visible = False
      	GR_OpenedArchive..Visible = False
      	GR_BottomMessage..Visible = False
      	
      	FileMenu.OpenFileMenu..State = Grayed
      	FileMenu.ExtractFileMenu..State = Grayed
      	FileMenu.DeleteFileMenu..State = Grayed
      	FileMenu.MenuFileProperties..State = Grayed
      	
      	RETURN
      	
      ELSE
      	
      	// If files are found, all the buttons are enabled and visible by default.
      	GR_Buttons..Visible = True
      	GR_OpenedArchive..Visible = True
      	GR_BottomMessage..Visible = True
      	
      	BTN_ADD..State = Active
      	BTN_DELETE..State = Active
      	BTN_DIVIDE..State = Active
      	BTN_MERGE..State = Active
      	
      	FileMenu.OpenFileMenu..State = Active
      	FileMenu.ExtractFileMenu..State = Active
      	FileMenu.DeleteFileMenu..State = Active
      	FileMenu.MenuFileProperties..State = Active
      END
      
      
      // According to special cases, some buttons are grayed/disabled
      
      // If the archive contains no files
      IF  zipNbFile(gMyArchive)=0 THEN
      	
      	GR_Buttons..Visible = False
      	BTN_ADD..Visible = True
      	
      	FileMenu.OpenFileMenu..State = Grayed
      	FileMenu.ExtractFileMenu..State = Grayed
      	FileMenu.DeleteFileMenu..State = Grayed
      	FileMenu.MenuFileProperties..State = Grayed
      END
      
      // If the archive is multi-file
      IF zipIsMulti(gMyArchive) THEN
      	// Multi-part archive: unable to split it, add and delete
      	BTN_DIVIDE..State = Grayed
      	BTN_ADD..State = Grayed
      	BTN_DELETE..State = Grayed
      	//	BTN_EXECUTABLE..State = grayed	
      	//	SelfExtractMenu..State = grayed
      ELSE
      	BTN_MERGE..State = Grayed	
      END
