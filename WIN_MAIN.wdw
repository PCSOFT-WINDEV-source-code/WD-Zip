      // Summary: Closes the archive currently opened
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE CloseArchive()
      // Procedure used to close the current archive if necessary
      
      // Close the opened archive (if any)
      IF zipExist(gMyArchive)=True THEN 
      	// No progress bar on zipClose therefore display a temporization window
      	OpenChild(WIN_WAIT_PROCESS,True)
      	
      	// Run the zip closed in parallel of the wait
      	ParaTask is a ParallelTask
      	ParaTask = ParallelTaskExecute(ZipCloseParallel)
      	WHILE ParallelTaskWait(ParaTask,20)=False
      		WinRedraw(WIN_WAIT_PROCESS)
      	END
      	
      	// Close the wait window
      	Close(WIN_WAIT_PROCESS)
      	
      	
      END
      
      
      TableDeleteAll(TABLE_ARCHIVE)
      
      // Reset the variables
      gfbExecutable = False
      gsArchive = ""
      
      // Remove the message from the status bar
      STC_MESSAGE1 = StringBuild(<§@163ca2ca012797500003§>, 0, LengthToString(0))
      STC_MESSAGE2 = StringBuild(<§@163ca2ca012797500004§>, 0, LengthToString(0))
      
      // Refresh the different buttons
      UpdStatusButtons()		
      
      // Update the title fo the window
      MyWindow..Title = ProjectInfo(piProjectName)
