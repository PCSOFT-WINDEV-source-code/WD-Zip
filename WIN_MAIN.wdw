         // Move in the architecture of archive folders? 
         IF TABLE_ARCHIVE[TABLE_ARCHIVE].COL_ICON.IMG_File = IMG_Directory THEN
         	// If first line => ".." go back
         	IF TABLE_ARCHIVE = 1 AND gnDisplayLevel>0  THEN
         		gnDisplayLevel -- 
         		// Save the current directory (which means remove the last directory from the current directory string
         		IF Contains(gsCurrentDirectoryPath,fSep)= True THEN
         			gsCurrentDirectoryPath = Left(gsCurrentDirectoryPath,(Length(gsCurrentDirectoryPath) - Length(ExtractString(gsCurrentDirectoryPath,1,fSep,FromEnd))-1))
         		ELSE
         			// If no fSep is found then we are on the first directory before the root, therefore the the path must be set to empty
         			gsCurrentDirectoryPath = ""
         		END
         	ELSE
         		gnDisplayLevel++
         		// Save the current directory
         		gsCurrentDirectoryPath+= [fSep]+TABLE_ARCHIVE[TABLE_ARCHIVE].COL_NAME
         	END
         	
         	STC_CurrentArchiveDirectory = gsCurrentDirectoryPath
         	
         	// Refresh the display
         	RefreshDisplay
         ELSE	
         	OpenFile()
         END
        type : 66051
