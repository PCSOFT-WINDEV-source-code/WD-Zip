      // Summary: Extracts files from an archive
      // Parameters:
      //	sForEmulating (ANSI string - default value=""): Path for extracting files
      // Return value:
      // 	None
      //
      PROCEDURE FileExtraction(sForControlling is string="")
      
      sDestination is string
      nRow is int
      sReturn is string
      sAttribute is string
      nAttribute is int
      bExtractKeepDir is boolean
      bAttributeNoAll, bAttributeYesAll are booleans
      arrFile is array of string
      
      
      // Check whether the archive exists
      IF zipExist(gMyArchive)=False THEN
      	Error(<§@163ca2ca012797500000§>)
      	RETURN
      END
      
      // If no file is selected
      IF TableSelectCount(TABLE_ARCHIVE)=0 THEN
      	IF YesNo(<§@163ca2ca012797500008§>, <§@163ca2ca012797500009§>) THEN
      		TableSelectPlus(TABLE_ARCHIVE)
      	ELSE
      		RETURN
      	END
      END
      
      
      // Select the extraction directory if necessary
      IF sForControlling="" THEN
      	// Select a directory and a extract mode
      	sReturn = Open(WIN_EXTRACTION_DIRECTORY)
      ELSE
      	sReturn = sForControlling
      END
      
      IF sReturn="" RETURN
      
      // Directory
      sDestination = CompleteDir(ExtractString(sReturn,1,TAB))
      // Mode
      bExtractKeepDir = ExtractString(sReturn,2,TAB)
      
      // If no destination directory
      IF sDestination="" THEN
      	Error(<§@163ca2ca01279750000a§>)
      	RETURN 
      END
      
      // extract without keeping the tree structure
      IF bExtractKeepDir=False
      	// Save the current directory
      	fCurrentDir(sDestination)
      END
      
      
      
      // For each selected line, check whether the file must be added to the extraction
      FOR ALL SELECTED ROW nRow of TABLE_ARCHIVE
      	nArchiveSubscript is int = TABLE_ARCHIVE[nRow].COL_ARCHIVE_SUBSCRIPT
      	
      	// Don't extract the directory but its content
      	IF TABLE_ARCHIVE[nRow].COL_ICON = IMG_Directory THEN
      		FOR EACH ArchiveFile of gMyArchive.File
      			IF Contains(ArchiveFile.Nom,TABLE_ARCHIVE[nRow].COL_NAME) THEN
      				// The file is already found in the directory, extract it
      				ArrayAdd(arrFile,ArchiveFile.Nom)
      			END
      		END
      	END
      	
      	
      	IF bAttributeYesAll=True THEN
      		ArrayAdd(arrFile,gMyArchive.File[nArchiveSubscript].Name)
      	ELSE
      		IF bAttributeNoAll=False THEN
      			
      			if nArchiveSubscript = 0 then 
      				continue
      			END
      			
      			sAttribute = gMyArchive.File[nArchiveSubscript].FileAttribute
      			nAttribute = Position(sAttribute, "R")+Position(sAttribute, "H")+Position(sAttribute, "U")
      			
      			IF nAttribute>0 THEN
      				SWITCH val(Open(WIN_YESNO, StringBuild(<§@163ca2ca01279750000b§>,gMyArchive.File[nArchiveSubscript].Name)))
      					CASE 1 // yes
      						ArrayAdd(arrFile,gMyArchive.File[nArchiveSubscript].Name)
      					CASE 2 // yes to all
      						bAttributeYesAll = True
      						ArrayAdd(arrFile,gMyArchive.File[nArchiveSubscript].Name)
      					CASE 3 // no
      						
      					CASE 4 // no to all
      						bAttributeNoAll = True
      						
      					CASE 5 // cancel
      						RETURN
      				END
      			ELSE
      				ArrayAdd(arrFile,gMyArchive.File[nArchiveSubscript].Name)
      			END
      		END
      	END
      END
      
      // Keep the directories?
      nOption is int
      IF bExtractKeepDir = True THEN
      	nOption = zipDirectory
      ELSE
      	nOption = zipNone
      END
      
      // Start of extraction
      HourGlass(True)
      PROGBAR_CurrentProcess..Visible = True
      PROGBAR_CurrentProcess = 0
      
      // Extract the files
      ArchiveExtractFiles(arrFile,sDestination,nOption,PROGBAR_CurrentProcess)
      
      // End of extraction
      HourGlass(False)
      PROGBAR_CurrentProcess..Visible  = False
