      // Summary: Returns the icon associated with the file passed in parameter
      // Parameters:
      // 	psFile: file whose icon must be found
      // Return value:
      // 	ANSI string: path to the icon
      //
      PROCEDURE GetAssociatedIcon(psFile)
      // retrieves the icon associated with a file
      
      nIconSubscript		is int
      nIconHandle		is int = 1
      nDC				is int
      nFileHandle	is int
      sFile			is string
      sIcon 			is string
      
      // create a temporary file with the appropriate extension
      sFile = fTempPath()+psFile
      
      nFileHandle = fCreate(sFile)
      IF nFileHandle<>-1  THEN
      	fClose(nFileHandle)
      ELSE
      	Error(ErrorInfo)	
      END
      
      // interrogate the associated icon
      nIconHandle = CallDLL32("Shell32.dll", "ExtractAssociatedIconA", SysInstance(), &sFile, &nIconSubscript)
      IF nIconHandle<>Null THEN
      	// Device Context of the control
      	nDC = dStartDrawing(IMG_ICON)
      	IF nDC<>0 THEN
      		// Draw the icon in an image control
      		CallDLL32("User32.dll", "DrawIcon", nDC, 0, 0, nIconHandle)
      	END
      END
      
      // save the icon
      sIcon = dSaveImagePNG(IMG_ICON, inMemory,White)
      
      // Freeing resources
      CallDLL32("User32.dll", "DestroyIcon", nIconHandle)
      
      // delete the temporary file
      fDelete(sFile)
      
      RESULT sIcon
