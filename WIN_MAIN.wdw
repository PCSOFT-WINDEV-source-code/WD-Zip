      // Summary: Delete the selected files from the archive
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE DeleteFiles()
      bDeletionConfirmed is boolean = False
      arrFile is array of strings
      sListFile is string
      
      
      
      IF gsArchive="" THEN
      	Error(<§$00a5§>)
      	RETURN
      END
      
      IF TableSelectCount(TABLE_ARCHIVE)=0 THEN
      	IF YesNo(<§$00a7§>)=Yes THEN
      		TableSelectPlus(TABLE_ARCHIVE)
      		bDeletionConfirmed = True
      	ELSE
      		RETURN
      	END
      END
      
      IF bDeletionConfirmed = False THEN
      	IF YesNo(<§$0071§>)= No THEN RETURN
      END
      
      
      i is int
      nline is int
      
      
      HourGlass(True)
      PROGBAR_CurrentProcess..Visible = True
      PROGBAR_CurrentProcess = 0
      
      // Multi-archive
      IF zipIsMulti(gMyArchive)=True THEN
      	
      	IF YesNo(<§$0084§>,<§$0085§>)=No THEN RETURN
      	
      	// save the selected rows
      	ListDeleteAll(LIST_HIDDEN)
      	FOR i=1 _TO_ TableCount(TABLE_ARCHIVE)
      		IF TABLE_ARCHIVE[i]..Selected THEN
      			ListAdd(LIST_HIDDEN,i)
      		END
      	END	
      	
      	// merge the multi-archives into a single archive
      	GroupArchive()
      	
      	// restores the selection
      	FOR i=1 _TO_ ListCount(LIST_HIDDEN)
      		TableSelectPlus(TABLE_ARCHIVE,LIST_HIDDEN[i])
      	END
      END
      
      // special case: if everything is selected (faster)
      IF TableSelectCount(TABLE_ARCHIVE)=TableCount(TABLE_ARCHIVE) THEN
      	// clear the archive
      	zipDeleteAll(gMyArchive)
      	TableDeleteAll(TABLE_ARCHIVE)
      	// Status of buttons
      	UpdStatusButtons()
      	
      	HourGlass(False)
      	PROGBAR_CurrentProcess..Visible = False
      	RETURN
      END
      
      // Add all the files to process
      FOR ALL SELECTED ROW nline of TABLE_ARCHIVE
      	ArrayAdd(arrFile,gMyArchive.File[TABLE_ARCHIVE[nline].COL_ARCHIVE_SUBSCRIPT].Name)
      END
      
      // Toggle the table to list
      sListFile = ArrayToString(arrFile,CR)
      
      IF zipDeleteFileList(gMyArchive,sListFile,PROGBAR_CurrentProcess)=0 THEN
      	RefreshDisplay()
      ELSE
      	Error("Error while deleting files.  Error: "+CR+ ErrorInfo(errFullDetails))
      END
      
      HourGlass(False)
      PROGBAR_CurrentProcess..Visible = False
