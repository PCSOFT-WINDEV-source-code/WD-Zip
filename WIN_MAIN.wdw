      IF gnCommandLineNumber>0 THEN
      	// Open the archive and display its content
      	IF OpenArchive(CommandLine(gnCommandLineNumber))= False AND geProcess=ActionOpen THEN
      		// If the application is started from a file but if this file cannot be opened, close the application
      		EndProgram()
      	END
      END
