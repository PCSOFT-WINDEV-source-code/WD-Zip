      // Summary: Refreshes the display according to the display mode
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE RefreshDisplay()
      
      // Empty archive? 
      IF gMyArchive..NbFile = 0 THEN
      	RETURN
      END
      
      PROGBAR_CurrentProcess..Visible =True
      TableDeleteAll(TABLE_ARCHIVE)
      PROGBAR_CurrentProcess = 0
      
      // Browse the archive files
      FOR i = 1 _TO_ gMyArchive..NbFile
      	DisplayArchiveFile(i)	
      	
      	// If we are not in directory mode, display a progress bar, the number of files to display may be important
      	IF gbDirectoryModeDisplay = False THEN
      		PROGBAR_CurrentProcess++	
      	END
      END
      
      
      // In directory mode, a ".." directory may be added to go back
      IF gbDirectoryModeDisplay= True  AND gnDisplayLevel > 0 THEN
      	TableInsertLine(TABLE_ARCHIVE,1,0, IMG_Directory, ...
      	"..",...
      	"",...
